
.fml-autoSelect{
  position: fixed;
  background: #eef1f9;
  width:650rpx;
  left:0;
  height: 100%;

}
.selectInput{
  padding:15rpx 23rpx;
  height:124rpx;
  border-bottom:1rpx solid #dcdcdc;
  position: relative;
 
}
.searchInput{
  background:white;
  padding-left:60rpx;
  font-size:24rpx;
   border-radius: 6px;
   height:2rem;
}
.iconSearch{
  position: absolute;
  top:50%;
  left:45rpx;
  margin-top:-6px;
}

.selectDetails{
  background:white;
  font-size:28rpx;
  border-bottom: 1rpx solid #dcdcdc;
  color:#333;
  
}
.selectHead{
  height:80rpx;
  display: flex;
  align-items: center;
  background:#eef1f9;  
  padding:0 23rpx;
}
.selectInfo{
  height:80rpx;
  border-bottom:2px solid #F6FAFE;
  display: flex;
  align-items: center;  
  padding:0 23rpx;
}
.selectInfo:last-child{
  border-bottom:none;
}

.iconCancel{
  position:absolute;
  right:10rpx;
  top:50%; 
  margin-top:-20px;
}